public class ClimbingStairsTabulation{
	public static int countWays(int n){
	 int fib[] = new int[n+1];
	 fib[0]=1;
	 fib[-1]=0;
	
	 for(int i=1;i<n+1;i++){
		 if(i==1)
			 fib[i] = fib[i-1];
		 else
	         fib[i]=fib[i-1]+fib[i-2]+fib[i-3];
	 }
		 return fib[n];
	}
	public static void main(String[] args){
		int n=5;
		System.out.println(countWays(n));
	}
}
